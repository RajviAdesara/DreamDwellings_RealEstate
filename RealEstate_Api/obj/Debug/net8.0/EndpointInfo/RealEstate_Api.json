  "components": {
    "schemas": {
      "AgentModel": {
        "required": [
          "agentName",
          "contactNumber",
          "experienceYears",
          "licenseNumber",
          "status"
        ],
        "type": "object",
        "properties": {
          "agentId": {
            "type": "integer",
            "format": "int32"
          },
          "agentName": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "licenseNumber": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "experienceYears": {
            "type": "integer",
            "format": "int32"
          },
          "contactNumber": {
            "maxLength": 15,
            "minLength": 0,
            "type": "string"
          },
          "officeAddress": {
            "maxLength": 255,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "profilePicture": {
            "type": "string",
            "nullable": true
          },
          "profilePicturePath": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "maxLength": 50,
            "minLength": 0,
            "pattern": "Active|Inactive|Suspended",
            "type": "string"
          }
        },
        "additionalProperties": false
      },
